{
  "expo": {
    "name": "trek",
    "slug": "trek",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.backyard.trek",
      "config": {
        "googleMapsApiKey": ""
      }
    },
    "android": {
      "package": "com.backyard.trek",
      "versionCode": 1,
      "config": {
        "googleMaps": { "apiKey": ""}
      }
    }
  }
}
